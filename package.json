{
  "name": "milanaryal.github.io",
  "title": "Milan Aryal",
  "url": "https://milanaryal.com.np",
  "description": "Build assets for milanaryal.com.np",
  "main": "index.js",
  "scripts": {
    "start": "npm run fresh && npm run build",
    "fresh": "rm -rf node_modules && npm install",
    "build": "npm-run-all clean css js",
    "clean": "rimraf assets/js/ assets/css/ _includes/static/",
    "css": "npm-run-all css-*",
    "css-compile": "node-sass --include-path node_modules --output-style compressed --precision 6 src/scss/main.scss -o assets/css/",
    "css-postcss": "NODE_ENV=production postcss assets/css/main.css -o assets/css/style.min.css && rimraf assets/css/main.css",
    "css-include": "rimraf _includes/static/style.min.css && cpy assets/css/style.min.css _includes/static/",
    "cssdev": "npm-run-all cssdev-*",
    "cssdev-clean": "rimraf assets/css/",
    "cssdev-compile": "node-sass --include-path node_modules --output-style expanded --precision 6 src/scss/main.scss -o assets/css/",
    "cssdev-postcss": "NODE_ENV=development postcss assets/css/main.css -o assets/css/main.css",
    "js": "webpack",
    "site": "npm-run-all site-*",
    "site-clean": "bundle exec jekyll clean",
    "site-compile": "bundle exec jekyll build --limit_posts 15 --incremental --config _config.yml,_config_dev.yml",
    "copy-css": "rimraf _site/assets/css/ && cpy assets/css/** _site/assets/css/",
    "copy-js": "rimraf _site/assets/js/ && cpy assets/js/** _site/assets/js/",
    "copy-fonts": "rimraf assets/fonts/ && cpy src/fonts/** assets/fonts/",
    "watch": "gulp watch",
    "lint": "npm-run-all lint-*",
    "lint-css": "stylelint \"**/*.{css,scss}\" --cache --cache-location .cache/.stylelintcache",
    "lint-js": "eslint --cache --cache-location .cache/.eslintcache --report-unused-disable-directives .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.13.0",
    "@primer/css": "14.3.0",
    "@primer/octicons": "9.6.0",
    "bootstrap": "4.5.0",
    "headroom.js": "0.11.0",
    "jquery": "3.5.1",
    "popper.js": "1.16.1",
    "rfs": "9.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "autoprefixer": "^9.8.0",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.7",
    "clean-webpack-plugin": "^3.0.0",
    "cpy-cli": "^3.1.1",
    "cssnano": "^4.1.10",
    "eslint": "^7.0.0",
    "eslint-config-xo": "^0.29.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-unicorn": "^20.0.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-discard-comments": "^4.0.2",
    "rimraf": "^3.0.2",
    "stylelint": "^13.5.0",
    "stylelint-config-twbs-bootstrap": "^2.0.2",
    "terser-webpack-plugin": "^3.0.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MilanAryal/milanaryal.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/MilanAryal/milanaryal.github.io/issues"
  },
  "homepage": "https://github.com/MilanAryal/milanaryal.github.io#readme",
  "author": "Milan Aryal",
  "license": "MIT",
  "private": true
}

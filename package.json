{
  "private": true,
  "name": "milanaryal.github.io",
  "title": "Milan Aryal",
  "url": "https://milanaryal.com.np",
  "description": "Build assets for milanaryal.com.np",
  "scripts": {
    "start": "npm run fresh && npm run build",
    "fresh": "rm -rf node_modules package-lock.json Gemfile.lock && npm install",
    "build": "npm-run-all clean css js",
    "clean": "rimraf \"src/assets/{css/,js/}\" \"src/_includes/css/style.css\"",
    "css": "npm-run-all css-*",
    "css-compile": "sass --load-path node_modules --style compressed --no-charset --no-source-map --no-error-css \"src/_assets/scss/style.scss\" \"src/assets/css/style.css\"",
    "css-postcss": "NODE_ENV=production postcss --replace \"src/assets/css/style.css\"",
    "css-include": "rimraf \"src/_includes/css/style.css\" && cpy \"src/assets/css/style.css\" \"src/_includes/css/\"",
    "cssdev": "npm-run-all cssdev-*",
    "cssdev-clean": "rimraf \"src/assets/css/**\"",
    "cssdev-compile": "sass --load-path node_modules --style expanded --no-charset --source-map --embed-sources --no-error-css \"src/_assets/scss/style.scss\" \"src/assets/css/style.css\"",
    "cssdev-postcss": "NODE_ENV=development postcss --replace \"src/assets/css/style.css\" --map",
    "js": "webpack",
    "copy-fonts": "rimraf \"src/assets/fonts/**\" && cpy \"src/_assets/fonts/**\" \"src/assets/fonts/\"",
    "site": "npm-run-all site-*",
    "site-clean": "bundle exec jekyll clean",
    "site-compile": "bundle exec jekyll build --limit_posts 15 --config _config.yml,_config_dev.yml --profile --trace",
    "copy:site-css": "rimraf \"_site/assets/css/**\" && cpy \"src/assets/css/**\" \"_site/assets/css/\"",
    "copy:site-js": "rimraf \"_site/assets/js/**\" && cpy \"src/assets/js/**\" \"_site/assets/js/\"",
    "watch": "npm-run-all --parallel watch-*",
    "watch-server": "npm run cssdev js && npm run site && browser-sync start --server \"./_site\" --port 4000 --no-ui --no-open",
    "watch-css": "chokidar \"src/_assets/scss/**/*.scss\" -c \"npm run cssdev && npm run copy:site-css && browser-sync reload --port 4000\"",
    "watch-js": "chokidar \"src/_assets/js/**/*.js\" -c \"npm run js && npm run copy:site-js && browser-sync reload --port 4000\"",
    "watch-site": "chokidar \"src/**/*.html\" \"src/**/*.md\" \"!src/_assets/**\" -c \"npm run site-compile && browser-sync reload --port 4000\"",
    "lint": "npm-run-all lint-*",
    "lint-css": "stylelint \"**/*.{css,scss}\" --cache --cache-location .cache/.stylelintcache || true",
    "lint-js": "eslint --cache --cache-location .cache/.eslintcache --report-unused-disable-directives . || true",
    "lint-md": "markdownlint-cli2 \"**/*.md\" \"!**/node_modules/**\" \"!src/_pages/style-guide.md\" \"!README.md\" || true",
    "lint-mdl": "bundle exec mdl --ignore-front-matter --style \"./.mdl_style.rb\" \"./src/_posts/\" || true",
    "lint-site": "JEKYLL_ENV=production bundle exec jekyll build --profile --trace && linkinator \"./_site\" --recurse --skip \"^(?!http://localhost)\" --verbosity error",
    "minify-icons": "minify --match=\\.svg --type=svg --recursive --output \"./src/_includes/icons/\" \"./src/_includes/icons/\" --verbose",
    "format": "prettier --write \"**/*.{js,scss,json,yml,md}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@popperjs/core": "2.9.2",
    "bootstrap": "5.0.0",
    "headroom.js": "0.12.0",
    "jquery": "3.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-env": "^7.14.1",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.14",
    "chokidar-cli": "^2.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cpy-cli": "^3.1.1",
    "cssnano": "^5.0.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-xo": "^0.36.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-unicorn": "^32.0.1",
    "linkinator": "^2.13.6",
    "markdownlint-cli2": "^0.1.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.15",
    "postcss-cli": "^8.3.1",
    "postcss-discard-comments": "^5.0.0",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "sass": "^1.32.12",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-twbs-bootstrap": "^2.2.0",
    "stylelint-prettier": "^1.2.0",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0"
  },
  "main": "_site/index.html",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MilanAryal/milanaryal.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/MilanAryal/milanaryal.github.io/issues"
  },
  "homepage": "https://github.com/MilanAryal/milanaryal.github.io#readme",
  "author": "Milan Aryal",
  "license": "MIT"
}

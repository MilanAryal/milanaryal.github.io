[{%- for post in site.posts limit:69 -%}{"t":"{{- post.title | smartify | strip_html | normalize_whitespace | escape_once -}}","u":"{{- post.url | replace:'/index.html','/' | relative_url -}}","d":"{{- post.date | date: '%b %-d, %Y' -}}","r":"{%- assign reading_time = post.content | strip_html | normalize_whitespace | number_of_words | plus:91 | divided_by:180 -%}{%- if reading_time <= 1 -%}{{- '1' | append:' min read' -}}{%- else -%}{{- reading_time | append:' min read' -}}{%- endif -%}","e":"{{- post.excerpt | markdownify | strip_html | normalize_whitespace | truncate: 80 | escape_once -}}"}{%- unless forloop.last -%},{%- endunless -%}{%- endfor -%}]
